{
  "name": "fontkit",
  "version": "1.0.3",
  "description": "An advanced font engine for Node and the browser",
  "keywords": [
    "opentype",
    "font",
    "typography",
    "subset",
    "emoji",
    "glyph",
    "layout"
  ],
  "scripts": {
    "test": "mocha",
    "coverage": "mocha --require coffee-coverage/register-istanbul && istanbul report",
    "prepublish": "coffee -c src/ *.coffee",
    "postpublish": "rm *.js && find src -name '*.js' -delete"
  },
  "author": "Devon Govett <devongovett@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "brfs": "^1.4.0",
    "brotli": "^1.0.0",
    "clone": "^1.0.1",
    "deep-equal": "^1.0.0",
    "iconv-lite": "^0.4.11",
    "pako": "^0.2.5",
    "restructure": "^0.5.0",
    "typedarray-to-buffer": "^3.0.0",
    "unicode-properties": "^1.0.0",
    "unicode-trie": "^0.3.0"
  },
  "devDependencies": {
    "codepoints": "^1.2.0",
    "coffee-coverage": "^0.4.2",
    "coffee-script": "^1.8.0",
    "concat-stream": "^1.4.6",
    "iconv-lite": "^0.4.7",
    "istanbul": "^0.3.17",
    "mocha": "^2.0.1"
  },
  "browserify": {
    "transform": [
      "brfs"
    ]
  }
}
